{
  "name": "foo1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env SUPPRESS=true tsx watch src/server/server.ts",
    "debug": "cross-env DEBUG=engine.io:* tsx watch src/server/server.ts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "dev:e2e": "cross-env NODE_ENV=test PLAYWRIGHT_TEST=1 PORT=4000 tsx src/server/server.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:playwright": "playwright test",
    "test:playwright:grep": "cross-env SUPPRESS=true playwright test --grep",
    "test:playwright:custom": "tsx tests/e2e/run-tests.ts",
    "test:all": "vitest --run --reporter=json --outputFile=test-results/unit-tests.json && playwright test --reporter=json > test-results/e2e-tests.json"
  },
  "dependencies": {
    "express": "^5.2.1",
    "framer-motion": "^12.23.26",
    "hangulx": "^0.6.5",
    "lodash": "^4.17.21",
    "next": "^16.0.7",
    "prom-client": "^15.1.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tsx": "^4.21.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "daisyui": "^5.5.14",
    "eslint": "^9",
    "eslint-config-next": "16.0.4",
    "jsdom": "^27.3.0",
    "msw": "^2.12.4",
    "rollup": "^4.54.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.16"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
